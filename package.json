{
  "name": "uerj-codeboard-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 del all",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "test": "vitest dev -w --ui --api 9547",
    "stress:user-flow": "cmd /c \"set K6_WEB_DASHBOARD=true && k6 run __tests__/stress/user-flow-test.js\""
  },
  "repository": "https://github.com/pitroldev/uerj-codeboard-server",
  "author": "Petro Cardoso <petrolcds@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.716.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.4",
    "morgan": "^1.10.0",
    "socket.io": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.24.1",
    "@types/bun": "^1.1.2",
    "@types/chance": "^1.1.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@vitest/ui": "^2.1.8",
    "babel-plugin-module-resolver": "^5.0.2",
    "chance": "^1.1.11",
    "mongodb-memory-server": "^9.5.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}